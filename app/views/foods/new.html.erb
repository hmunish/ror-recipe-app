<h1>New Food</h1>







<%= form_with(model: @food, url: user_foods_path) do |form| %>

    <div class="form-food">




  <div class="label">

    <%= form.label :name %>

    <%= form.text_field :name %>

  </div>

  <div class="label">

    <%= form.label :measurement_unit %> 

    <%= form.text_field :measurement_unit %>

  </div>

  <div class="label">

    <%= form.label :price %> 

    <%= form.text_field :price %>

  </div>

  <div class="label">

    <%= form.label :quantity %> 

    <%= form.text_field :quantity %>

  </div>

  <div class="food-btn">

    <p><%= form.submit "Create Food", class: "new-food-btn" %></p>

  </div>

  </div>

<% end %>